{
  "manifest_version": 2,

  "name": "Wizz",
  "description": "Wizz",
  "version": "0.0.0.3",
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "http://*/*",
    "https://*/*",
    "<all_urls>"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["https://wizzair.com/*"],
      "js": ["jquery.js"]
    }
  ],
  "icons": { "16": "icon16.png",
			 "48": "icon48.png",
			 "128": "icon128.png" },

  "browser_action": {
    "default_popup": "popup.html"
  },
			 
  "options_page": "options.html" // settings page
}